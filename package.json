{
  "name": "code.svg",
  "version": "0.6.6",
  "description": "A JavaScript library to manipulate the SVG DOM in a object-oriented way.",
  "main": "codesvg.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "mocha ./tests",
    "test-travis": "./node_modules/istanbul/lib/cli.js cover -x '**/grammars/**' -x '**/node-particles/**' ./node_modules/mocha/bin/_mocha -- -R spec ./tests/",
    "gendocs": "jsdoc -r src -d docs/out/ -t docs/jaguarjs-jsdoc/"
  },
  "readmeFilename": "README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/figurebelow/code.svg.git"
  },
  "keywords": [
    "svg",
    "nodejs",
    "d3",
    "art",
    "generative"
  ],
  "author": "Ruben <ruben@figurebelow.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/figurebelow/code.svg/issues"
  },
  "devDependencies": {
    "coveralls": "^2.11.6",
    "istanbul": "^0.4.2",
    "mocha": "2.4.5",
    "jsdoc": "^3.4.0"
  },
  "homepage": "https://github.com/figurebelow/code.svg#readme",
  "dependencies": {
    "body-parser": "^1.15.2",
    "compressjs": "^1.0.3",
    "sync-request": "^3.0.1"
  }
}
