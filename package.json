{
  "name": "code.svg",
  "version": "0.8.0",
  "description": "A JavaScript library to manipulate the SVG DOM in a object-oriented way.",
  "main": "codesvg.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "mocha ./tests",
    "test-travis": "./node_modules/istanbul/lib/cli.js cover -x '**/grammars/**' -x '**/node-particles/**' ./node_modules/mocha/bin/_mocha -- -R spec ./tests/",
    "gendocs": "./node_modules/.bin/jsdoc -r src -d docs/out/ -t docs/template/"
  },
  "readmeFilename": "README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rubenafo/code.svg.git"
  },
  "keywords": [
    "svg",
    "nodejs",
    "d3",
    "art",
    "generative",
    "particles"
  ],
  "author": "Ruben Afonso <rbfrancos@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/rubenafo/code.svg/issues"
  },
  "devDependencies": {
    "coveralls": "^2.11.6",
    "istanbul": "^0.4.2",
    "mocha": "2.4.5",
    "jsdoc": "^3.4.0"
  },
  "homepage": "https://github.com/rubenafo/code.svg#readme",
  "dependencies": {
    "body-parser": "^1.15.2",
    "chroma-js": "^1.3.6",
    "colourlovers": "0.1.1",
    "compressjs": "^1.0.3",
    "d3": "^4.0.0",
    "nodemon": "^1.17.1",
    "npm": "^5.7.1",
    "peg": "0.0.2",
    "phantomjs": "^2.1.7",
    "pn": "^1.1.0",
    "svg2png": "^4.1.1",
    "sync-request": "^3.0.1",
    "xmldom": "^0.1.27"
  }
}
